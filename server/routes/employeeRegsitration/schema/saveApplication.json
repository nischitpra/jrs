{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "email",
    "age",
    "sex",
    "department",
    "position"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "nischit"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "a@a.com"
      ],
      "pattern": "^(.*)$"
    },
    "age": {
      "$id": "#/properties/age",
      "type": "integer",
      "title": "The Age Schema",
      "default": "",
      "examples": [
        "23"
      ]
    },
    "sex": {
      "$id": "#/properties/sex",
      "type": "string",
      "title": "The Sex Schema",
      "default": "",
      "examples": [
        "m"
      ],
      "pattern": "^(.*)$"
    },
    "department": {
      "$id": "#/properties/department",
      "type": "string",
      "title": "The Department Schema",
      "default": "",
      "examples": [
        "Trading"
      ],
      "pattern": "^(.*)$"
    },
    "position": {
      "$id": "#/properties/position",
      "type": "string",
      "title": "The Position Schema",
      "default": "",
      "examples": [
        "Employee"
      ],
      "pattern": "^(.*)$"
    }
  }
}