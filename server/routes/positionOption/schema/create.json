{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "position_level",
    "department"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Manager"
      ],
      "pattern": "^(.*)$"
    },
    "position_level": {
      "$id": "#/properties/position_level",
      "type": "integer",
      "title": "The Position_level Schema",
      "default": "",
      "examples": [
        "3"
      ],
      "pattern": "^(.*)$"
    },
    "department": {
      "$id": "#/properties/department",
      "type": "string",
      "title": "The Department Schema",
      "default": "",
      "examples": [
        "Trading"
      ],
      "pattern": "^(.*)$"
    }
  }
}