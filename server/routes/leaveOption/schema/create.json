{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "max",
    "type"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Personal"
      ],
      "pattern": "^(.*)$"
    },
    "max": {
      "$id": "#/properties/max",
      "type": "integer",
      "title": "The Max Schema",
      "default": "",
      "examples": [
        "6"
      ],
      "pattern": "^(.*)$"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "accumulating"
      ],
      "pattern": "^(.*)$"
    }
  }
}