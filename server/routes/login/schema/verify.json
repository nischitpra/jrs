{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "employee_id",
    "password"
  ],
  "properties": {
    "employee_id": {
      "$id": "#/properties/employee_id",
      "type": "integer",
      "title": "The Employee_id Schema",
      "default": "",
      "examples": [
        "1"
      ]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The Password Schema",
      "default": "",
      "examples": [
        "hello"
      ],
      "pattern": "^(.*)$"
    }
  }
}